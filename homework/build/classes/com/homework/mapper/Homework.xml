<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.homework.mapper.HomeworkMapper">
	<insert id="add" parameterType="Homework">
		insert into homework (
		stuId,stuName,
		homeWorkTitle,homeWorkPath,homeWorkSubmitTime,homeWorkIndex) values
		(#{stuId},#{stuName},
		#{homeWorkTitle},#{homeWorkPath},#{homeWorkSubmitTime},#{homeWorkIndex})
	</insert>
	<select id="searchMyHomeWork" parameterType="hashmap"
		resultType="Homework">
		select * from
		homework where stuId = #{stuId} and stuName =
		#{stuName}
	</select>
	<!-- <delete id="delete" parameterType="Homework"> delete from category_ 
		where id= #{id} </delete> <select id="get" parameterType="_int" resultType="Homework"> 
		select * from category_ where id= #{id} </select> <update id="update" parameterType="Homework"> 
		update category_ set name=#{name} where id=#{id} </update> -->
	<select id="list" resultType="Homework">
		select * from homework
	</select>
</mapper>

